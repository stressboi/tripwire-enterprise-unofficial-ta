        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Most Changed Element by Host</title>
        <searchString>index=te | stats sparkline count by "Node Name","Element Name" | sort -count</searchString>
        <earliestTime>0</earliestTime>
        <latestTime/>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Top Windows System Binary Changes</title>
        <searchString>index=te "Rule Name"="Windows - System Binaries" | stats sparkline count by "Element Name" | sort -count</searchString>
        <earliestTime>0</earliestTime>
        <latestTime/>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Changes by file/users</title>
        <searchString>index=te| eval Users=if(Users="","UNKNOWN",Users) | stats dc(ElementName) as fileschanged,values(Users) as users by NodeName | sort -fileschanged</searchString>
        <earliestTime/>
        <latestTime/>
      </table>
    </panel>
  </row>
</dashboard>